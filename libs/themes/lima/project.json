{
	"root": "libs/themes/lima",
	"sourceRoot": "libs/themes/lima/src",
	"targets": {
		"build": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/themes/lima",
				"main": "libs/themes/lima/src/index.ts",
				"tsConfig": "libs/themes/lima/tsconfig.lib.json",
				"assets": ["libs/themes/lima/*.md"]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/themes/lima/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/themes/lima"],
			"options": {
				"jestConfig": "libs/themes/lima/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": ["type:ui"],
	"implicitDependencies": ["@rhenag/"]
}
